                <if expression="too1%too2=0">
                    <then>
                        <assign variable="x" expression="x+1"/>
                    </then>
                    <else/>
                </if>
            </for>
            <if expression="x=2">
                <then>
                    <output expression="&quot;prime number&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;not prime number&quot;" newline="True"/>
                </else>
            </if>
        </body>
