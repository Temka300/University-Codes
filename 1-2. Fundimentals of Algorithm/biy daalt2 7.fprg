    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="too1, too2, x" type="Integer" array="False" size=""/>
            <assign variable="x" expression="0"/>
            <output expression="&quot;&#1059;&#1090;&#1075;&#1072;&#1072; &#1086;&#1088;&#1091;&#1091;&#1083;&#1085;&#1072; &#1091;&#1091;&quot;" newline="True"/>
            <input variable="too1"/>
